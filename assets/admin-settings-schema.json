{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "hidden_data"
  ],
  "properties": {
    "hidden_data": {
      "$id": "#/properties/hidden_data",
      "type": "object",
      "title": "The Hidden_data Schema",
      "required": [
        "payment_options",
        "sort",
        "sandbox",
        "statement_descriptor"
      ],
      "properties": {
        "payment_options": {
          "$id": "#/properties/hidden_data/properties/payment_options",
          "type": "array",
          "title": "The Payment_options Schema",
          "items": {
            "$id": "#/properties/hidden_data/properties/payment_options/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "name",
              "type",
              "payment_type",
              "installments",
              "intermediator",
              "icon",
              "url"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Cart√£o Credito"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/type",
                "type": "string",
                "title": "The Type Schema",
                "default": "",
                "examples": [
                  "credit_card"
                ],
                "pattern": "^(.*)$"
              },
              "payment_type": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/payment_type",
                "type": "string",
                "title": "The Payment_type Schema",
                "default": "",
                "examples": [
                  "payment"
                ],
                "pattern": "^(.*)$"
              },
              "installments": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/installments",
                "type": "array",
                "title": "The Installments Schema",
                "items": {
                  "$id": "#/properties/hidden_data/properties/payment_options/items/properties/installments/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "number",
                    "tax",
                    "tax_value"
                  ],
                  "properties": {
                    "number": {
                      "$id": "#/properties/hidden_data/properties/payment_options/items/properties/installments/items/properties/number",
                      "type": "integer",
                      "title": "The Number Schema",
                      "default": 0,
                      "examples": [
                        12
                      ]
                    },
                    "tax": {
                      "$id": "#/properties/hidden_data/properties/payment_options/items/properties/installments/items/properties/tax",
                      "type": "boolean",
                      "title": "The Tax Schema",
                      "default": false,
                      "examples": [
                        true
                      ]
                    },
                    "tax_value": {
                      "$id": "#/properties/hidden_data/properties/payment_options/items/properties/installments/items/properties/tax_value",
                      "type": "integer",
                      "title": "The Tax_value Schema",
                      "default": 0,
                      "examples": [
                        50
                      ]
                    }
                  }
                }
              },
              "intermediator": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/intermediator",
                "type": "object",
                "title": "The Intermediator Schema",
                "required": [
                  "code",
                  "link",
                  "name"
                ],
                "properties": {
                  "code": {
                    "$id": "#/properties/hidden_data/properties/payment_options/items/properties/intermediator/properties/code",
                    "type": "string",
                    "title": "The Code Schema",
                    "default": "",
                    "examples": [
                      "wirecard"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "link": {
                    "$id": "#/properties/hidden_data/properties/payment_options/items/properties/intermediator/properties/link",
                    "type": "string",
                    "title": "The Link Schema",
                    "default": "",
                    "examples": [
                      "https://www.wirecard.com.br"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "name": {
                    "$id": "#/properties/hidden_data/properties/payment_options/items/properties/intermediator/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "default": "",
                    "examples": [
                      "Wirecard"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "icon": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/icon",
                "type": "string",
                "title": "The Icon Schema",
                "default": "",
                "examples": [
                  "https://ecom.nyc3.digitaloceanspaces.com/plus/assets/img/stamps/wirecard.png"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/hidden_data/properties/payment_options/items/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "https://www.wiredcard.com.br/payment"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "sort": {
          "$id": "#/properties/hidden_data/properties/sort",
          "type": "array",
          "title": "The Sort Schema",
          "items": {
            "$id": "#/properties/hidden_data/properties/sort/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "online_debit",
              "credit_card",
              "banking_billet"
            ],
            "pattern": "^(.*)$"
          }
        },
        "sandbox": {
          "$id": "#/properties/hidden_data/properties/sandbox",
          "type": "boolean",
          "title": "The Sandbox Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "statement_descriptor": {
          "$id": "#/properties/hidden_data/properties/statement_descriptor",
          "type": "string",
          "title": "The Statement_descriptor Schema",
          "default": "",
          "examples": [
            "Loja teste"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}